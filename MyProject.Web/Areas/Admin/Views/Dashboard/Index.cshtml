                                            @*<img src="~/Assets/Admin/images/dashboard/dashboardicons/positions open.png" />*@
                                            <i class="fas fa-building fa-2x stats-color"></i>
                                        </span>
                                        <div class="text-dark font-weight-bolder font-size-h4 mt-3 t-propert-types">0</div>
                                        <p class="text-muted text-hover-dark font-weight-bold font-size-lg mt-1">Property Types</p>
                                    </div>
                                </a>
                            </div>
