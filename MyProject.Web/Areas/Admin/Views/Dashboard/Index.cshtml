                                <a @*href="/Admin/Property/Index"*@>
                                    <div class="card-body">
                                        <span class="menu-icon">
                                            @*<img src="~/Assets/Admin/images/dashboard/dashboardicons/positions open.png" />*@
                                            <i class="fas fa-house-user fa-2x stats-color"></i>
                                        </span>
                                        <div class="text-dark font-weight-bolder font-size-h4 mt-3 t-properties">0</div>
                                        <p class="text-muted text-hover-dark font-weight-bold font-size-lg mt-1">Properties</p>
                                    </div>
                                </a>
                            </div>
                            <!--end::Tiles Widget 11-->
                        </div>
                    </div>
				</div>
			</div>
			<!--end::Row cards-->
			<!--begin::Row charts-->
			<div class="row" hidden>
				<div class="col-lg-6 col-xxl-4">
					<!--begin::Mixed Widget 4-->
					<div class="card card-custom bg-warning-o-60 gutter-b card-stretch">
						<!--begin::Header-->
						<div class="card-header border-0 py-5">
							<h3 class="card-title font-weight-bolder">Total Earn for this Year</h3>
						</div>
						<!--end::Header-->
						<!--begin::Body-->
						<div class="card-body d-flex flex-column p-0">
							<!--begin::Chart-->
							<div id="sales_chart" style="height: 200px; margin: 5px;"></div>
							<!--end::Chart-->
							<!--begin::Stats-->
							<div class="card-spacer bg-white card-rounded flex-grow-1">
								<!--begin::Row-->
                                <div class="row m-0">
                                    <div class="col px-8 py-6 mr-8">
                                        <div class="font-size-sm text-center text-muted font-weight-bold">Bookings</div>
                                        <div class="font-size-h4 text-center font-weight-bolder">@String.Format("{0:G29}", decimal.Parse(Model.Stats.CompletedBookings.ToString()))</div>
                                    </div>
                                    <div class="col px-8 py-6">
                                            <div class="justify-content-center" style="display: flex;">
                                                <span><i class="fa fa-circle font-size-sm" style="color: #58b1d0;"></i></span>
                                                <div class="font-size-sm text-center text-muted font-weight-bold pl-1" style="padding-top: 1px;">Total Amount</div>
                                            </div>
                                        <div class="font-size-h4 text-center font-weight-bolder" id="total_amount_sum"></div>
                                    </div>
                                    <div class="col px-8 py-6">
                                        <div class="justify-content-center" style="display: flex;">
                                            <span><i class="fa fa-circle font-size-sm" style="color: #bc9359;"></i></span>
                                            <div class="font-size-sm text-center text-muted font-weight-bold pl-1" style="padding-top: 1px;">Net Amount</div>
                                        </div>
                                        <div class="font-size-h4 text-center font-weight-bolder" id="net_amount_sum"></div>
                                    </div>
                                </div>
								<!--end::Row-->
