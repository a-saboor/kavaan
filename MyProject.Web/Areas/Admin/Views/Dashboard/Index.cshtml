							<!--begin::Chart-->
							<div id="bookings_chart" style="height: 200px; margin: 5px;"></div>
							<!--end::Chart-->
							<!--begin::Stats-->
							<div class="card-spacer bg-white card-rounded flex-grow-1">
								<!--begin::Row-->
