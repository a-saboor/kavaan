@model MyProject.Web.Areas.Admin.ViewModels.DashboardStatsViewModel
@{
	ViewBag.Title = "Home Page";
	Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<!--begin::Page Vendors Styles(used by this page)-->
<link href="~/assets/Admin/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" type="text/css" />
<!--end::Page Vendors Styles-->
<link href="~/Assets/Admin/css/admin/dashboard/index.css" rel="stylesheet" />
<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid dashboard">
	<!--begin::Subheader-->
	<div class="subheader py-2 py-lg-12 subheader-transparent" id="kt_subheader">
		<div class="container d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
			<!--begin::Info-->
			<div class="d-flex align-items-center flex-wrap mr-1">
				<!--begin::Heading-->
				<div class="d-flex flex-column">
					<!--begin::Title-->
					<h2 class="text-white font-weight-bold my-2 mr-5">Admin Dashboard</h2>
					<!--end::Title-->
					<!--begin::Breadcrumb-->
					<div class="d-flex align-items-center font-weight-bold my-2">
						<!--begin::Item-->
						<a href="javascript:;" class="text-white text-hover-white opacity-75 hover-opacity-100">Welcome to Kavaan</a>
						<!--end::Item-->
					</div>
					<!--end::Breadcrumb-->
				</div>
				<!--end::Heading-->
			</div>
			<!--end::Info-->
		</div>
	</div>

	<div class="d-flex flex-column-fluid">
		<!--begin::Container-->
		<div class="container">
			<!--begin::Dashboard-->
			<!--begin::Date Filter-->
			<div class="pb-10 row search_filter_row">
				<div class="form-group mb-1 col-sm-6 col-md-5">
					<label class="text-white text-hover-white opacity-75 hover-opacity-100">From Date</label>
					<div class="input-daterange input-group kt_datepicker_range">
						@Html.Editor("FromDate", new { htmlAttributes = new { @class = "form-control kt_datepicker_range", type = "text", id = "fromDate", placeholder = " MM/DD/YYYY", required = "required" } })
						<div class="input-group-append"><span class="input-group-text"><i class="fa fa-calendar"></i></span></div>

					</div>
				</div>
				<div class="form-group mb-1 col-sm-6 col-md-5">
					<label class="text-white text-hover-white opacity-75 hover-opacity-100">To Date</label>
					<div class="input-daterange input-group kt_datepicker_range">
						@Html.Editor("ToDate", new { htmlAttributes = new { @class = "form-control kt_datepicker_range", type = "text", id = "toDate", placeholder = " MM/DD/YYYY", required = "required" } })
						<div class="input-group-append"><span class="input-group-text"><i class="fa fa-calendar"></i></span></div>
					</div>
				</div>
				<div class="form-group mb-1 col-sm-6 col-md-2">
					<label></label>
					<br>
					<button type="button" style="margin-top: 6px" class="btn btn-block p-3 btn-custom" id="btnSearch" onclick="SearchFilter(this,'/Admin/Dashboard/Filter')"><i class="fa fa-filter text-white text-white"></i>Filter</button>
				</div>
				<br />
			</div>
			<!--end::Date Filter-->
			<!--begin::Row cards-->
			<div class="row">
				<div class="col-md-12 sm-12 col-xxl-12">
                    <div class="row">
                        <div class="col-xl-2">
                            <!--begin::Tiles Widget 11-->
                            <div class="card card-custom gutter-b bg-gutter-1 bgi-size-cover bgi-no-repeat" style="height: 150px;">
                                <a href="/Admin/User/Index">
                                    <div class="card-body">
                                        <span class="menu-icon">
                                            @*<img src="~/Assets/Admin/images/dashboard/dashboardicons/positions open.png" />*@
                                            <i class="fas fa-users fa-2x stats-color"></i>
                                        </span>
                                        <div class="text-dark font-weight-bolder font-size-h4 mt-3 t-users">@String.Format("{0:G29}", decimal.Parse(Model.Stats.TotalUsers.ToString()))</div>
                                        <p class="text-muted text-hover-dark font-weight-bold font-size-lg mt-1">Users</p>
                                    </div>
                                </a>
                            </div>
                            <!--end::Tiles Widget 11-->
                        </div>
                        <div class="col-xl-2">
                            <!--begin::Tiles Widget 11-->
                            <div class="card card-custom gutter-b bg-gutter-1 bgi-size-cover bgi-no-repeat" style="height: 150px;">
