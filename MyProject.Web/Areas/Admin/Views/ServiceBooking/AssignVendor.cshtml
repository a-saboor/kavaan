@model MyProject.Data.ServiceBooking

@{
    Layout = null;
}
<link href="~/Assets/Admin/plugins/custom/richtextarea/rte_theme_default.css" rel="stylesheet" />

<div class="modal-header">
    <h5 class="modal-title">Assign Vendor</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <i aria-hidden="true" class="ki ki-close"></i>
    </button>
</div>

<div class="modal-body">
    <form id="createform" action="/Admin/ServiceBooking/AssignVendor/" name="form" enctype="multipart/form-data" class="form-file-custom" onsubmit="return form_file_custom(this)">
        @Html.AntiForgeryToken()
        <div class="alert alert-danger text-center" id="divMessage" style="display:none"></div>
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="row">
            <div class="col-md-12 col-sm-12">
                <div class="form-group">
                    <label class="control-label">Vendor</label>
                    <div class="input-group mb-2">
                        @Html.DropDownList("VendorID", null, "Select Vendor", htmlAttributes: new { @class = "form-control txtdisabled", @id = "VendorID" })
                        <div class="input-group-append"><span class="input-group-text"><i class="fa fa-server"></i></span></div>
                    </div>
                    @Html.ValidationMessageFor(model => model.VendorID, "", new { @class = "text-danger" })
                </div>
            </div>
            <input value="@ViewBag.ID" name="ID" hidden/>
        </div>
    </form>
</div>

<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
    <button type="submit" class="btn btn-custom" form="createform">Assign</button>
</div>

<script>


</script>
<!--end::Page Scripts-->
<script src="~/Assets/Admin/plugins/custom/richtextarea/rte.js"></script>
<script src="~/Assets/Admin/plugins/custom/richtextarea/all_plugins.js"></script>
<script src="~/Assets/Admin/plugins/custom/richtextarea/rte-upload.js"></script>


