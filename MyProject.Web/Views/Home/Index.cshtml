                            <h1 class="section-title">Property By Categories</h1>
                        </div>
                    </div>
                </div>
                <div class="row">


                    @{ int PTIdx = 0;}
                    @foreach (var item in Model.ProjectTypes)
                    {
                        PTIdx++;
                        if (PTIdx == 1)
                        {
                            <div class="col-lg-8 col-md-6">
                                <div class="ltn__banner-item ltn__banner-style-4 text-color-white bg-image" data-bs-bg="@Url.Content(item.Projects.FirstOrDefault().Thumbnail)">
                                    <div class="ltn__banner-info">
                                        <h3><a href="~/projects?type=@item.ID"> @item.Name </a></h3>
                                        <p> Great Deals Available</p>
                                        <mark> @item.Projects.Count() Listings</mark>
                                    </div>
                                </div>
                            </div>
                        }
                        else
                        {
                            <div class="col-lg-4 col-md-6">
